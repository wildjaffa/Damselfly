{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DamselflyConfiguration": {
    "SourceDirectory": "PathToFiles",
    "ThumbPath": "PathToFiles",
    "DatabasePath": "PathToDB",
    "LogPath": "PathToLogs",
    "DownloadPath": "PathToTemporaryLocationForDownloads",
    "Verbose": false,
    "Trace": false,
    "Readonly": false,
    "Port": 6363,
    "Synology": false,
    "NoGenerateThumbnails": false,
    "EnableIndexing": false,
    "UsePostgresDB": false
  },
  "ImageMagick": {
    // https://imagemagick.org/script/download.php
    "ExePath": "PathToExe"
  },
  "ExifTool": {
    "ExePath": "PathToExe"
  },
  "MailGun": {
    "Domain": "mailgunDomain",
    "ApiKey": "MailGunApiKey",
    "FromAddress": "FromAddressForMailGun"
  },
  "ContactForm": {
    "ToAddress": "emailAddressToSendContactRequests"
  },
  "jwt": {
    "Firebase": {
      "ValidIssuer": "https://securetoken.google.com/{FIREBASEID}",
      "ValidAudience": "FIREBASEID"
    }
  },
  "AdminEmails": "CommaSeperatedListOfAdmins"
}
